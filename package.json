{
  "name": "@hiv3d/harvest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:main && npm run build:content",
    "build:main": "tsc -b && vite build",
    "build:content": "npm run build:tiktok && npm run build:tiktok-debug && npm run build:amazon && npm run build:thangs && npm run build:background && npm run build:patreon && npm run build:makerworld",
    "build:tiktok": "vite build --config vite.content.config.ts --mode tiktok",
    "build:tiktok-debug": "vite build --config vite.content.config.ts --mode tiktok-debug",
    "build:amazon": "vite build --config vite.content.config.ts --mode amazon",
    "build:thangs": "vite build --config vite.content.config.ts --mode thangs",
    "build:background": "vite build --config vite.content.config.ts --mode background",
    "build:patreon": "vite build --config vite.content.config.ts --mode patreon",
    "build:makerworld": "vite build --config vite.content.config.ts --mode makerworld",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.49.9",
    "@tanstack/react-query": "^5.67.3",
    "@types/chrome": "^0.0.287",
    "@types/node": "^22.13.10",
    "@types/react-dom": "^19.0.0",
    "@types/react": "^19.0.10",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.10",
    "eslint": "^9.29.0",
    "lucide-react": "^0.471.1",
    "postcss": "^8.5.1",
    "react-dom": "19.0.0",
    "react": "19.0.0",
    "sharp": "^0.32.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite-plugin-static-copy": "^2.3.0",
    "vite": "^6.2.2"
  },
  "devDependencies": {

  }
}
